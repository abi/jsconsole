{
  "name": "@remy/jsconsole",
  "version": "2.1.2",
  "engines": {
    "node": "16.x.x"
  },
  "now": {
    "alias": "2.jsconsole.com"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "parser": "babylon",
    "printWidth": 80,
    "proseWrap": "preserve",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "babylon": "^6.18.0",
    "babylon-walk": "^1.0.2",
    "classnames": "^2.2.5",
    "copy-to-clipboard": "^3.0.8",
    "is-url": "^1.2.4",
    "lodash": "^4.17.4",
    "preact": "^8.2.5",
    "prismjs": "^1.25.0",
    "prop-types": "^15.5.10",
    "react-copy-to-clipboard": "^5.0.0",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-simple-code-editor": "^0.11.0",
    "redux": "^3.7.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@remy/eslint": "^1.3.0",
    "autoprefixer": "10.4.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "8.2.3",
    "babel-runtime": "6.23.0",
    "body-parser": "^1.17.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "6.5.1",
    "dotenv": "4.0.0",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.6.2",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "^21.2.1",
    "node-uuid": "^1.4.8",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "6.2.1",
    "postcss-preset-env": "^7.1.0",
    "preact-compat": "^3.17.0",
    "prettier": "1.10.2",
    "prettier-eslint": "^8.7.4",
    "promise": "7.1.1",
    "react": "^15.5.4",
    "react-dev-utils": "^3.1.1",
    "react-error-overlay": "^1.0.10",
    "react-test-renderer": "^16.1.1",
    "redbox-react": "^1.5.0",
    "redux-devtools": "^3.4.0",
    "style-loader": "3.3.1",
    "svg-inline-loader": "^0.8.2",
    "sw-precache-webpack-plugin": "0.11.3",
    "url-loader": "0.5.9",
    "webpack": "5.65.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "3.0.0",
    "webpack-manifest-plugin": "1.1.0"
  },
  "_proxy": "http://localhost:3001",
  "scripts": {
    "env": "env",
    "start": "REACT_APP_VERSION=$npm_package_version node scripts/start.js",
    "build": "rm -rf build; PUBLIC_URL=/ REACT_APP_VERSION=$npm_package_version node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "styles": "cat src/*.css > src/core/jsconsole.css",
    "watch": "nodemon -w src/ -e css -x 'npm run styles'",
    "lib": "rm -rf dist; npm run styles; NODE_ENV=production babel src/core --out-dir dist --copy-files --ignore '__tests__'",
    "deploy": "npm run build && s3_website push",
    "prepare": "npm run lib",
    "web": "rm -rf build; npm run styles; NODE_ENV=production BABEL_ENV=production webpack"
  },
  "main": "build/app.bundle.js",
  "files": [
    "dist/"
  ],
  "jest": {
    "modulePathIgnorePatterns": [
      "object-assign"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "__eslintConfig": {
    "extends": "react-app"
  },
  "description": "Web based console - for presentations, workshops and jsbin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remy/jsconsole.git"
  },
  "keywords": [],
  "author": "Remy Sharp <remy@leftlogic.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/remy/jsconsole/issues"
  },
  "homepage": "https://github.com/remy/jsconsole#readme"
}
